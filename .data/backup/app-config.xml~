<!--####################################################################################-->
<!--## Filename    : app-config.xml                                                   ##-->
<!--## Author      : Sampath Singamsetty, Nrusing Nayak (Middleware Engineering)      ##-->
<!--## Owner       : Raj Damera / Somesh Adiraju                                      ##-->
<!--## Version     : 1.3                                                              ##-->
<!--## Description :                                                                  ##-->
<!--##               This configuration file contains application specific settings.  ##-->
<!--##               relevant  for each  individual service  or end-point. All of the ##-->
<!--##               normal requests going to the backend specified in the MPG config ##-->
<!--##               which does not need any special  interception is not required to ##-->
<!--##               be specified here.                                               ##-->
<!--##               Only those services which needs to be sent to  different backend ##-->
<!--##               than the original MPG backend or those services  which needs any ##-->
<!--##               special treatment like request or response transformations etc., ##-->
<!--##               needs to be specified here.                                      ##-->
<!--##               The transformations which triggers this configuration file which ##-->
<!--##               in the present case is svc-config.xsl checks the request url and ##-->
<!--##               looks for an entry in this  file. If it finds an entry, the same ##-->
<!--##               will be populated as  internal http request  headers so that the ##-->
<!--##               information propogates from one object (MPG) to other object.    ##-->
<!--##               This configuration set is  augmented by 2 additional MPG objects ##-->
<!--##               which needs to be configured on Datapower as below               ##-->
<!--##               (i)  XML Transformer (For processing xml data to json)           ##-->
<!--##               (ii) JSON Transformer (For processing the json data)             ##-->
<!--##                                                                                ##-->
<!--##               Each service node set in the configuration file specifis below:  ##-->
<!--##               uri - a matching uri for which interception is needed            ##-->
<!--##               sslProfile - if backend url is over https specify the object     ##-->
<!--##               reqTransform - if any transformation required for request data   ##-->
<!--##               resTransform - if any transformation required for response data  ##-->
<!--##               proxyUrl - the FSH address of XML or JSON Transformer MPG(s)     ##-->
<!--##               routingUrl - the final destination url of the service            ##-->
<!--####################################################################################-->
<app-metadata>
    <!--********************************************************************-->
    <!--** below are all for the production services                      **-->
    <!--********************************************************************-->
    <service uri="/csr/mapCSRAPI/mapcsr_service/login/session/host/status">
        <sslProfile>client:SSL_CLIENT_ROOT</sslProfile>
        <reqTransform/>
        <resTransform>LoginSessionStatusJSONx.xsl</resTransform>
        <proxyUrl>http://PublicInterfaceINETFrontEnd:3486</proxyUrl>
        <routingUrl>https://unitedservices.ual.com/6.0/security/authentication/login/session/host/status</routingUrl>
    </service>
    <service uri="/csr/mapCSRAPI/mapcsr_service/mobileSvc/ApsAssociateDeviceToBpPrinterGetLeid">
        <sslProfile/>
        <reqTransform>AssociateDeviceToBpPrinterGetLeidReqSOAP.xsl</reqTransform>
        <resTransform>AssociateToBpPrinterGetLeidResJSONx.xsl</resTransform>
        <proxyUrl>http://PublicInterfaceINETFrontEnd:3486</proxyUrl>
        <routingUrl>http://apsmanagement.ual.com/APSMobile/APSMobileService.svc</routingUrl>
    </service>
    <service uri="/csr/mapCSRAPI/mapcsr_service/mobileSvc/ApsGetWorkstationLeid">
